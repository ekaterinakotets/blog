    <!-- home section -->
    <div id="home" class="section  home-bg">
        <div class="container full-height-section flex-centered">
            <div class="col-md-4">
                <!-- booking div -->
                <form id="contact-form" name="contact" method="post" novalidate="novalidate" class="booking-div" data-aos="fade-down" data-aos-delay="200">

                    <div class="form-group">
                        <input type="text" id="name" name="name" class="form-control" minlength="2" placeholder="Enter your name" />
                    </div>

                    <div class="form-group">
                        <input type="text" id="time" minlength="2" name="time" class="form-control flatpickr" placeholder="Select Day" data-enable-time="true" />
                    </div>

                    <div class="form-group">
                        <select name="type" class="form-control">
                            <option>Select Service</option>
                            <option>The second option</option>
                            <option>The third option</option>
                            <option>The second option</option>
                            <option>The third option</option>
                        </select>
                    </div>

                    <div class="form-group">
                        <input type="tel" name="phone" class="form-control" placeholder="Phone number" />
                    </div>
                    <div class="form-group">
                        <input type="email" name="email" class="form-control" placeholder="E-mail" />
                    </div>


                    <div class="form-group">
                        <button id="submit" type="submit" name="submit" class=" pull-right btn btn-brown btn-md">Book now</button>
                    </div>

                    <div id="success">

                        <p class="green textcenter">
                           <%= image_tag "checked.svg", :style => "width: 100px; height: 100px" %> Your message was sent successfully! I will be in touch as soon as I can.
                        </p> 
                    </div>

                    <div id="error">
                        <p>
                            <%= image_tag "close.svg", :style => 'width: 50px' %> Something went wrong, try refreshing and submitting the form again.
                        </p>
                    </div>
                </form>
                <!--/ .booking div -->


            </div>
