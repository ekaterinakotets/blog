<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Riva Booking Template</title>
    <link rel="icon" href="img/favicon.ico" type="image/x-icon">

    <!-- Bootstrap -->
    <link href="assets/bootstrap.min.css" rel="stylesheet">
    <!-- icon -->
    <link href="assets/ionicons.min.css" rel="stylesheet">
    <link href="assets/aos.css" rel="stylesheet">
    <link href="assets/owl.carousel.css" rel="stylesheet">
    <link href="assets/flatpickr.min.css" rel="stylesheet">
    <!-- icon -->


    <!-- main style -->
    <link href="assets/style.css" rel="stylesheet">

    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>

<body id="top">

    <!-- Preloader -->
    <div id="preloader">
        <div id="status">
            <div class="preloader">
                <div>
                    <div></div>
                    <div></div>
                    <div></div>
                    <div></div>
                </div>
            </div>
        </div>
    </div>
    <!-- navbar -->
   <%= render 'layouts/header' %>
    <!--/ .navbar -->

    <!-- navbar -->
    <div class="title-bar">
        <div class="container">
            <h2>News Archive</h2>
            <p><%= link_to "Home", root_path %><span>/</span> News Archive </p>
        </div>
    </div>
   
    <div class="container main-container">
        <div class="col-md-8">
            <div class="main-blog">
                <div id="main">
                  <% @paginator.items.each do |post| %>
                    <%= render partial: 'post_overview', locals: {post: post} %>
                  <% end %>
                </div>

                <!-- navigation -->
                <nav aria-label="Page navigation" class="navigation">
                    <ul class="pagination pagination-lg">
                        <li>
                            <a href="#" aria-label="Previous">
                                <span aria-hidden="true">&laquo;</span>
                            </a>
                        </li>
                        <li class="active"><%= render partial: 'paginator' %></li>
                        <li>
                            <a href="#" aria-label="Next">
                                <span aria-hidden="true">&raquo;</span>
                            </a>
                        </li>
                    </ul>
                </nav>
                <!--/ .navigation -->

            </div>
        </div>

    <%= render 'blogo/shared/aside' %>
    </div>
   
    <!-- footer -->
   <%= render 'layouts/footer' %>
    <!--/ .footer -->









    <!-- jQuery -->
    <script src="assets/jquery.js"></script>
    <!-- plugins -->
    <script src="assets/bootstrap.min.js"></script>
    <script src="assets/jquery.form.js"></script>
    <script src="assets/jquery.validate.min.js"></script>
    <script src="assets/aos.js"></script>
    <script src="assets/owl.carousel.min.js"></script>
    <script src="assets/flatpickr.min.js"></script>
    <script src="assets/swiper.jquery.min.js"></script>
    <script src="assets/isotope.pkgd.min.js"></script>
    <script src="assets/jquery.magnific-popup.min.js"></script>
    <script src="assets/custom.js"></script>






</body>

</html>